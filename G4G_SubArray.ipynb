class PreCase(object):    
    def __init__(self,N=0,S=0,NT=0):
        self.N = N
        self.S = S
        self.NT = NT
    def inputNS(self):
        self.N,self.S = input().strip().split()
        self.N=int(self.N)
        self.S=int(self.S)
    def inputNT(self):
        self.NT = input().strip().split()
        for ite2 in range(self.N):
            self.NT[ite2] = int(self.NT[ite2])
